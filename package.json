{
  "name": "connected-next-router",
  "version": "1.0.2",
  "description": "A Redux binding for Next.js Router",
  "main": "lib/index.js",
  "author": "Daniel Reinoso <danielreinoso1807@gmail.com> (http://danielr18.github.io)",
  "license": "MIT",
  "keywords": [
    "next",
    "next.js",
    "next-router",
    "next-router-redux",
    "connected-react-router",
    "react-router-redux"
  ],
  "files": [
    "actions.js",
    "ConnectedRouter.js",
    "index.js",
    "middleware.js",
    "patchRouter.js",
    "reducer.js",
    "routerMethods.js",
    "utils",
    "structure",
    "es"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "cross-env BABEL_ENV=commonjs babel src -d . --extensions \".ts,.tsx\"",
    "build:es": "cross-env BABEL_ENV=es babel src -d es --extensions \".ts,.tsx\"",
    "watch": "cross-env BABEL_ENV=commonjs babel src -d . -w --extensions \".ts,.tsx\"",
    "clean": "git clean -xdfe node_modules",
    "lint": "tsc && eslint './src/**/*.{ts,tsx}'"
  },
  "peerDependencies": {
    "next": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "react": "^16.8.0",
    "react-redux": "^6.0.0",
    "redux": "^3.6.0 || ^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/node": "^12.6.2",
    "@types/react": "^16.8.23",
    "@types/react-redux": "^7.1.1",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "next": "^9.0.1",
    "prop-types": "^15.5.8",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielr18/connected-next-router.git"
  },
  "bugs": {
    "url": "https://github.com/danielr18/connected-next-router/issues"
  }
}
